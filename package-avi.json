{
  "name": "cms-ui",
  "version": "0.1.0",
  "description": "New analytics project using webpack to build & deploy",
  "main": "src/app/app.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "npm run test && NODE_ENV=development node_modules/.bin/webpack-dev-server -d --hot --inline --colors --content-base src/",
    "start:notest": "NODE_ENV=development node_modules/.bin/webpack-dev-server -d --hot --inline --colors --content-base src/",
    "start:notest:win": "set NODE_ENV=development && webpack-dev-server -d --port 7575 --hot --inline --colors --content-base src/",
    "deploy": "NODE_ENV=production node_modules/.bin/webpack -p --optimize-dedupe",
    "deploy:win": "set NODE_ENV=production && webpack -p --optimize-dedupe",
    "deploy-debug": "NODE_ENV=production_debug node_modules/.bin/webpack -p --devtool source-map",
    "deploy-integ": "npm run deploy && npm run deploy-debug &&  sshpass -p '123qwe@' scp -r dist root@LTV01:/exchange/cms-ui",
    "test": "NODE_ENV=test karma start",
    "test:live": "NODE_ENV=test karma start --auto-watch --no-single-run"
  },
  "author": "Avi Engelshtein",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.6.12",
    "babel-loader": "^5.2.2",
    "css-loader": "^0.14.5",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-war": "0.0.1",
    "gulp-zip": "^3.0.2",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^2.6.1",
    "image-webpack-loader": "^1.6.1",
    "imagemin": "^3.2.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.37",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "0.0.10",
    "ngtemplate-loader": "^1.3.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.4.0-beta1",
    "raw-loader": "^0.5.1",
    "sass-loader": "git+https://github.com/Avien/sass-loader",
    "sprity": "^1.0.7",
    "sprity-lwip": "^1.0.6",
    "sprity-sass": "^1.0.4",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "html-loader": "^0.4.0",
    "jquery": "^2.1.4"
  }
}
