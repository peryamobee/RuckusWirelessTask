{
  "name": "timer-app",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "config": {
    "port": "3000"
  },
  "scripts": {
    "test": "set NODE_ENV=test karma start",
    "test:live": "set NODE_ENV=test && karma start --auto-watch --no-single-run",
    "build": "webpack --colors --watch --optimize-dedupe",
    "start": "set NODE_ENV=development && webpack-dev-server -d --hot --port 4040",
    "deploy": "set NODE_ENV=production && webpack -p --optimize-dedupe",
    "deploy:debug": "set NODE_ENV=production && webpack -p --devtool source-map"
  },
  "dependencies": {
    "angular": "^1.4.9",
    "animate.css": "^3.4.0",
    "jquery": "^2.2.0",
    "moment": "^2.11.1",
    "moment-duration-format": "^1.3.0"
  },
  "devDependencies": {
    "angular-ui-router.statehelper": "^1.3.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "html-webpack-plugin": "^2.6.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-sourcemap-loader": "^0.3.5",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "require-dir": "^0.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ui-router": "^0.2.15",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.1.0"
  },
  "author": "",
  "license": "ISC"
}
